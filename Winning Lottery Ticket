#include <stdio.h>
#include <string.h>

int digitMask(char *ticket) {
    int mask = 0;
    for (int i = 0; ticket[i]; i++) {
        int digit = ticket[i] - '0';
        mask |= (1 << digit);
    }
    return mask;
}

int winningLotteryTicket(char tickets[][100], int n) {
    int masks[n];
    for (int i = 0; i < n; i++) {
        masks[i] = digitMask(tickets[i]);
    }

    int fullMask = (1 << 10) - 1; // 0b1111111111 (digits 0 to 9)
    int count = 0;

    for (int i = 0; i < n; i++) {
        for (int j = i + 1; j < n; j++) {
            if ((masks[i] | masks[j]) == fullMask) {
                count++;
            }
        }
    }
    return count;
}

int main() {
    int n;
    scanf("%d", &n);

    char tickets[n][100];
    for (int i = 0; i < n; i++) {
        scanf("%s", tickets[i]);
    }

    int result = winningLotteryTicket(tickets, n);
    printf("%d\n", result);

    return 0;
}

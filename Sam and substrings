#include <stdio.h>
#include <string.h>

#define MOD 1000000007

long long substrings(char* n) {
    int len = strlen(n);
    long long total = 0;
    long long prev = 0;

    for (int i = 0; i < len; i++) {
        int digit = n[i] - '0';
        prev = (prev * 10 + digit * (i + 1)) % MOD;
        total = (total + prev) % MOD;
    }

    return total;
}

int main() {
    char n[100001];
    scanf("%s", n);
    printf("%lld\n", substrings(n));
    return 0;
}

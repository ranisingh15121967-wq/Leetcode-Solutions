#include <stdio.h>
#include <stdlib.h>

int compare(const void *a, const void *b) {
    return (*(int *)a - *(int *)b);
}

void stones(int n, int a, int b) {
    if (a > b) {
        int temp = a;
        a = b;
        b = temp;
    }

    int result[n + 1];
    for (int i = 0; i <= n; i++) {
        result[i] = i * b + (n - i) * a;
    }

    qsort(result, n + 1, sizeof(int), compare);

    printf("%d", result[0]);
    for (int i = 1; i <= n; i++) {
        if (result[i] != result[i - 1]) {
            printf(" %d", result[i]);
        }
    }
    printf("\n");
}

int main() {
    int T;
    scanf("%d", &T);

    while (T--) {
        int n, a, b;
        scanf("%d", &n);
        scanf("%d", &a);
        scanf("%d", &b);
        stones(n - 1, a, b);
    }

    return 0;
}
